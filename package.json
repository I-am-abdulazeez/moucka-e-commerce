{
  "name": "mouka-ecommerce-portal-backend",
  "version": "1.0.0",
  "description": "E-commerce Portal For Mouka ",
  "main": "server.js",
  "scripts": {
    "build": "cd client && npm run build",
    "client-install": "cd client && npm install",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build",
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Codeplus",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.15"
  }
}
